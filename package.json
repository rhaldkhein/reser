{
  "name": "reser",
  "version": "0.0.0",
  "description": "An asynchronous DI framework for modular React and React-Native",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run clean && babel src --out-dir lib",
    "watch-basic": "npm run clean && babel src --watch --out-dir sample/basic/src/reser",
    "watch-advance": "npm run clean && babel src --watch --out-dir sample/advance/src/reser",
    "clean": "rm -rf lib sample/basic/src/reser sample/advance/src/reser",
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhaldkhein/reser.git"
  },
  "author": "RhaldKhein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhaldkhein/reser/issues"
  },
  "homepage": "https://github.com/rhaldkhein/reser#readme",
  "files": [
    "services.js",
    "lib",
    "src"
  ],
  "dependencies": {
    "jservice": "^0.1.33",
    "lodash.set": "^4.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-es2016": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "eslint": "^5.16.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "node-localstorage": "^1.3.1",
    "react": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4"
  },
  "keywords": [
    "react",
    "react native",
    "framework",
    "modular",
    "services",
    "di",
    "ioc",
    "di container",
    "dependency injection"
  ]
}